.section-box
    .container 
        .panel-white
            .panel-head 
                h6.text-xl-bold.neutral-1000 Latest Tour Booking
                a.menudrop#dropdownMenu3(type='button', data-bs-toggle='dropdown', aria-expanded='false', data-bs-display='static')
                ul.dropdown-menu.dropdown-menu-light.dropdown-menu-end(aria-labelledby='dropdownMenu3')
                    li
                        a.dropdown-item.active(href='#') Add new
                    li
                        a.dropdown-item(href='#') Settings
                    li
                        a.dropdown-item(href='#') Actions
            .panel-body
                .box-list-tours.list-tours.wow.fadeIn
                    -
                        const itemLists = [
                            {
                                index: "1",
                                image: "assets/imgs/page/tours/tour1.png",
                                title: 'From Paris: Day Trip to Champagne with 8 Tastings & Lunch',
                                duration: '2 days, 3 nights',
                                passenger: '6-8 guest',
                                price: '$48.25',
                                link: '#'
                            },
                            {
                                index: "2",
                                image: "assets/imgs/page/tours/tour2.png",
                                title: 'From Paris: Day Trip to Champagne with 8 Tastings & Lunch',
                                duration: '2 days, 3 nights',
                                passenger: '6-8 guest',
                                price: '$48.25',
                                link: '#'
                            }
                        ]
                    each item in itemLists
                        .card-journey-small.background-card 
                            .card-image 
                                a.wish(href="#")
                                    svg(width='20', height='18', viewbox='0 0 20 18', fill='none', xmlns='http://www.w3.org/2000/svg')
                                        path(d='M17.071 10.1422L11.4141 15.7991C10.6331 16.5801 9.36672 16.5801 8.58568 15.7991L2.92882 10.1422C0.9762 8.1896 0.9762 5.02378 2.92882 3.07116C4.88144 1.11853 8.04727 1.11853 9.99989 3.07116C11.9525 1.11853 15.1183 1.11853 17.071 3.07116C19.0236 5.02378 19.0236 8.1896 17.071 10.1422Z', stroke='', stroke-width='1.5', stroke-linecap='round', stroke-linejoin='round')
                                img(src= item.image, alt="Travila")
                            .card-info.background-card 
                                .card-rating 
                                    .card-left 
                                    .card-right 
                                        span.rating 4.96 
                                            span.text-sm-medium.neutral-500 (672 reviews)
                                .card-title 
                                    a.text-lg-bold.neutral-1000(href= item.link)= item.title 
                                .card-program 
                                    .card-duration-tour 
                                        p.icon-duration.text-sm-medium.neutral-500= item.duration
                                        p.icon-guest.text-sm-medium.neutral-500= item.passenger
                                    .endtime 
                                        .card-price 
                                            h6.heading-6.neutral-1000= item.price
                                        .card-button 
                                            a.btn.btn-gray(href="#") Book Now
                include ../../elements/pagination.pug